{
  "expo": {
    "name": "myApp",
    "slug": "myApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/myLogo.jpg",
    "splash": {
      "image": "./assets/myLog.png",
      "resizeMode": "contain",
      "backgroundColor": "#b0e0e6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.maheshpabeykoon.myApp"
    },
    "web": {
      "favicon": "./assets/adaptive-icon.png"
    }
  }
}